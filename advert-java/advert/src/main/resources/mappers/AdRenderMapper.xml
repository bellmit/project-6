<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.miguan.advert.domain.mapper.AdRenderMapper" >

    <select id="findRNameByRKey" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT r_name rander_name
        from ad_render ar
        where r_key = #{value} limit 1
    </select>

</mapper>